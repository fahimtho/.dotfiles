[
  {
    "name": "1. Daily comic",
    "command": "xkcd"
  },
  {
    "name": "2. Random comic",
    "command": "xkcd -r"
  },
  {
    "name": "3. TODOs",
    "command": "notify-send 'TODOs' 'What do you want do ?' && kitty --class=TODO -1 taskwarrior-tui"
  },

  {
    "name": "4. Restart music",
    "command": "notify-send 'Behold' 'Restarting Music !!!' && pkill mopidy && mopidy"
  },
  {
    "name": "5. Restart Dunst",
    "command": "notify-send 'Behold' 'Restarting Notification Deamon' && pkill dunst && ~/.config/dunst/reload"
  },
  {
    "name": "6. Fetch database",
    "command": "notify-send 'Fetching' 'Package Database' && yay -Sy"
  },
  {
    "name": "7. Tmux Sessions",
    "command": "notify-send 'Tmux' 'Tinker with sessions' && ~/.scripts/tmux.sh"
  },
  
  {
    "name": "8. Things to Read",
    "command": "notify-send 'Things to Read' 'What do you wanna read ?' && ~/.scripts/things_to_read.sh"
  },
{
    "name": "9. Books",
    "command": "notify-send 'Books' 'Only Friends' && ~/.scripts/books.sh"
  },
{
    "name": "10. Latex Notes",
    "command": "notify-send 'Notes' 'Written in Latex' && ~/.scripts/notes_latex.sh"
  },
{
    "name": "11. Markdown Notes",
    "command": "notify-send 'Notes' 'Written in Markdown' && ~/.scripts/notes_markdown.sh"
  },
{
    "name": "12. Pdf Notes",
    "command": "notify-send 'Notes' 'Compiled notes in PDF' && ~/.scripts/notes_pdf.sh"
  }

]
